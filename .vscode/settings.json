{
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "autoBarrel.formatting.useSingleQuotes": true,
  "cSpell.enableFiletypes": [
    "!asciidoc",
    "!c",
    "!cpp",
    "!csharp",
    "!go",
    "!handlebars",
    "!haskell",
    "!jade",
    "!java",
    "!javascriptreact",
    "!jupyter",
    "!latex",
    "!less",
    "!php",
    "!pug",
    "!python",
    "!restructuredtext",
    "!rust",
    "!scala",
    "!scss",
    "!swift",
    "!vue",
    "!dockercompose",
    "dotenv",
    "!git-rebase",
    "!graphql",
    "ignore",
    "log",
    "!makefile",
    "!shellscript",
    "svg",
    "tailwindcss",
    "toml",
    "typescript"
  ],
  "cSpell.language": "en-US",
  "cSpell.words": [
    "abjad",
    "Addeds",
    "arbitrum",
    "Autorenew",
    "browserslistrc",
    "bunx",
    "chartjs",
    "codegen",
    "commitlint",
    "consts",
    "copperx",
    "cssnano",
    "Dismissable",
    "endregion",
    "flexbox",
    "flexbugs",
    "googletagmanager",
    "hookform",
    "ipfs",
    "keccak",
    "listbox",
    "Luganodes",
    "neobase",
    "neue",
    "nextjs",
    "nextui",
    "noopener",
    "noreferrer",
    "nosniff",
    "packagejson",
    "parens",
    "progressbar",
    "rainbowkit",
    "Sepolia",
    "sophon",
    "stylelint",
    "svgr",
    "tailwindcss",
    "tanstack",
    "undelegated",
    "Useds",
    "viem",
    "wagmi",
    "walletconnect",
    "walletlink",
    "webp",
    "zustand"
  ],
  "css.validate": false,
  "editor.autoClosingQuotes": "always",
  "editor.codeActionsOnSave": {
    "source.fixAll": "always",
    "source.organizeImports": "always"
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.linkedEditing": true,
  "editor.quickSuggestions": {
    "strings": true
  },
  "eslint.lintTask.enable": true,
  "eslint.probe": ["javascript", "typescript", "typescriptreact"],
  "files.autoSave": "afterDelay",
  "files.eol": "\n",
  "files.exclude": {
    "**/.git": true,
    "**/.vscode": true,
    "**/node_modules": true,
    "**/.next": true
  },
  "files.insertFinalNewline": true,
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/*/**": true,
    "**/.next/**": true
  },
  "git.autoStash": true,
  "git.autofetch": true,
  "git.closeDiffOnOperation": true,
  "git.enableSmartCommit": true,
  "git.repositoryScanIgnoredFolders": [
    "node_modules",
    ".next",
    ".husky",
    ".vscode",
    ".next",
    "types",
    "public",
    "src"
  ],
  "githubIssues.createIssueTriggers": ["TODO", "BUG", "FIX"],
  "githubPullRequests.defaultMergeMethod": "squash",
  "githubPullRequests.ignoredPullRequestBranches": ["main", "staging"],
  "javascript.suggestionActions.enabled": false,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "prettier.documentSelectors": ["**/*.svg"],
  "search.exclude": {
    "**/node_modules": true,
    "**/.next": true
  },
  "tailwindCSS.files.exclude": ["**/.git/**", "**/node_modules/**", "**/.hg/**", "**/.next/**"],
  "todo-tree.filtering.excludeGlobs": ["node_modules", ".next"],
  "todo-tree.general.automaticGitRefreshInterval": 60,
  "todo-tree.general.tags": ["BUG", "FIX", "TODO"],
  "todo-tree.highlights.backgroundColourScheme": ["red", "orange", "blue"],
  "todo-tree.highlights.foregroundColourScheme": ["white", "white", "white"],
  "todo-tree.highlights.useColourScheme": true,
  "typescript.preferences.quoteStyle": "single",
  "typescript.tsdk": "node_modules/typescript/lib"
}
